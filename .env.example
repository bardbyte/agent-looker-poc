# DMP-SL-Agent Environment Configuration
# Copy this file to .env and fill in your values

# ============================================================================
# Enterprise Authentication (IdaaS)
# ============================================================================
IDAAS_CLIENT_ID=your-client-id
IDAAS_CLIENT_SECRET=your-client-secret

# ============================================================================
# LLM Configuration
# ============================================================================
MODEL_API_KEY=your-model-api-key
MODEL_ID=gemini-pro

# ============================================================================
# Looker MCP Configuration
# ============================================================================
LOOKER_BASE_URL=https://yourcompany.looker.com
LOOKER_CLIENT_ID=your-looker-client-id
LOOKER_CLIENT_SECRET=your-looker-client-secret
LOOKER_VERIFY_SSL=true

# ============================================================================
# Optional Settings
# ============================================================================
LOG_LEVEL=INFO
MAX_ITERATIONS=15
